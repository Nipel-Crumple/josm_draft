{
		"classname":"filters.UnsharpMaskFilter",
		"binaries":["lib/unsharpmask.jar"],
        "name":"sharpen",
        "title": "Sharpen / Contrast",
        "controls": [
                {
                        "type": "linear_slider",
                        "name": "amount",
                        "title": "Amount",
                        "value_type": "float",
                        "scale": [0, 1],
                        "default": 0.2,
                        "decoration": "none"
                },
                {
                        "type": "linear_slider",
                        "name": "size",
                        "title": "Size (px)",
                        "value_type": "integer",
                        "scale": [0, 20],
                        "default": 2,
                        "decoration": "none"
                }
        ]
}